<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<paletteDefinition>
	<content>
		<drawer iconpath="/icons/f.gif" id="simpleFlowchart_Drawer" name="simpleFlowchart">
			<aspectTool description="Create a Flowchart (Class)"
				iconpath="platform:/plugin/autoGenedProf/icons/f.gif"
				id="clazz.tool.class_Flowchart_Flowchart"
				name="Flowchart (Class)"
				refToolId="clazz.tool.class">
				<postAction id="org.eclipse.papyrus.applystereotypeactionprovider">
					<stereotypesToApply>
						<stereotype stereotypeName="RootElement::Flowchart" />
					</stereotypesToApply>
				</postAction>
			</aspectTool>
			<aspectTool description="Create a Node (Class)"
				iconpath="platform:/plugin/autoGenedProf/icons/f.gif"
				id="clazz.tool.class_Node_Node"
				name="Node (Class)"
				refToolId="clazz.tool.class">
