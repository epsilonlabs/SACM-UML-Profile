					<stereotypesToApply>
						<stereotype stereotypeName="RootElement::AnotherNode" />
					</stereotypesToApply>
				</postAction>
			</aspectTool>
				<aspectTool description="Create a nodeClass2Association (Association)"
				iconpath="platform:/plugin/autoGenedProf/icons/f.gif"
				id="clazz.tool.association_nodeClass2Association_nodeClass2Association"
				name="nodeClass2Association (Association)"
				refToolId="clazz.tool.association">
				<postAction id="org.eclipse.papyrus.applystereotypeactionprovider">
					<stereotypesToApply>
						<stereotype stereotypeName="RootElement::nodeClass2Association" />
					</stereotypesToApply>
